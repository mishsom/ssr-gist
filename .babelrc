{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "env": {
    "server": {
      "plugins": [
        [
          "css-modules-transform",
          {
            "generateScopedName": "[hash:8]",
            "extensions": [
              ".css"
            ]
          }
        ],
        [
          "react-css-modules",
          {
            "handleMissingStyleName": "ignore"
          }
        ]
      ]
    }
  }
}